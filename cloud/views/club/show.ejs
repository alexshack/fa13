<!-- Include the header partial and specify a title -->
<% title = club.get('name') %>
<% include ../_header %>

<!-- content -->
<div id="page_content">
        <div id="page_content_inner">
            <div class="uk-grid" data-uk-grid-margin data-uk-grid-match id="user_profile">
                           <div class="uk-width-large-3-10">
                    <div class="md-card">
                        <div class="user_heading">
                            <div class="user_heading_menu" data-uk-dropdown>
                                <i class="md-icon material-icons md-icon-light">&#xE5D4;</i>
                                <div class="uk-dropdown uk-dropdown-flip uk-dropdown-small">
                                    <ul class="uk-nav">
                                        <li><a href="#">В избранное</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="user_heading_avatar">
                                <img src="http://www.fa13.info/image/logo/<%= club.get('clubId') %>.gif" alt="user avatar"/>
                            </div>
                            <div class="user_heading_content">
                                <h1 class="heading_b uk-margin-bottom"><span class="uk-text-truncate"><%= club.get('name') %></span>
                                <a href="#" class="sub-heading"><%= club.get('city') %>
                                <a href="#" class="sub-heading"><img class="flag flag-nl" />  <%= club.get('country') %></a></h1>

 
                            </div>
                        </div>
                        <div class="md-card-content">
                            <ul class="md-list md-list-addon">
                                <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">star_rate</i>
                                    </div>
                                    <div class="md-list-content">
                                        <div class="uk-float-right uk-margin-top uk-margin-small-right"><span class="peity_sale peity_data">5,3,9,6,5,9,7,3,5,2</span></div>
                                        
                                        <h2 class="uk-margin-remove"><%= club.get('raiting') %> <span class="uk-float-none uk-badge uk-badge-success uk-badge-notification">+1</span></h2>
                                        <span class="uk-text-small uk-text-muted">Рейтинг</span>

                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">attach_money</i>
                                    </div>
                                    <div class="md-list-content">
                                        <div class="uk-float-right uk-margin-top uk-margin-small-right"><span class="peity_sale peity_data">-100,-98,-105,-80,-56,-80,-42,-22,2,17</span></div>
                                        <h2 class="uk-margin-remove"><span class="countUpMe"><%= club.get('fc') %></span><span class="uk-badge uk-badge-success uk-badge-notification">+1329000</span></h2>
                                        <span class="uk-text-small uk-text-muted">Финансы клуба</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">swap_vert</i>
                                    </div>
                                    <div class="md-list-content">
                                        <div class="uk-float-right uk-margin-top uk-margin-small-right"><span class="peity_sale peity_data">152,150,187,198,190,195,193,205,202,225</span></div>
                                        <h2 class="uk-margin-remove"><span class="countUpMe"><%= club.get('bum') %></span></h2>
                                        <span class="uk-text-small uk-text-muted">БУМ</span>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>

                </div>

                <div class="uk-width-large-7-10">
                    <div class="md-card">

                        <div class="user_content">
                            <ul id="user_profile_tabs" class="uk-tab" data-uk-tab="{connect:'#user_profile_tabs_content', animation:'slide-horizontal'}" data-uk-sticky="{ top: 48, media: 960 }">
                                <li class="uk-active"><a href="#">Состав</a></li>
                                <li><a href="#">Матчи</a></li>
                                <li><a href="#">Статистика</a></li>
                            </ul>
                            <ul id="user_profile_tabs_content" class="uk-switcher uk-margin">
                                <li>
                                    <table class="uk-table uk-table-nowrap">
                            <thead>
                            <tr>
                                <th class="uk-text-right">#</th>
                                <th class="uk-text-center"">Поз </th>
                                <th class="uk-width-2-10">Игрок</th>
                                <th class="uk-text-center">Воз </th>
                                <th class="uk-text-center">Тал </th>

                                <th class="uk-text-center">Опыт</th>
                                <th class="uk-text-center">Сила</th>
                                <th class="uk-text-center">Физа</th>
                                <th class="uk-text-center">Дух </th>
                                <th class="uk-text-center">Здр </th>
                                <th class="uk-text-center">Сыг </th>
                                <th class="uk-width-2-10 uk-text-center">СВ</th>
                                <th class="uk-text-right">ЗП  </th>
                                <th class="uk-text-right">Цена</th>
                                <th class="uk-text-right">Тур </th>
                            </tr>
                            </thead>
                            <tbody>
                            <% players.forEach(function(players) { %>
                                <tr>
                                    <td class="uk-text-right"><%= players.get("number") %></td>
                                    <td class="uk-text-center""><%= players.get("position") %></td>
                                    <td><a href="/player/<%= players.get("playerId") %>"><%= players.get("name") %></a></td>
                                    <td class="uk-text-center"><%= players.get("age") %></td>
                                    <td class="uk-text-center"><%= players.get("talent") %></td>
                                    <td class="uk-text-center"><%= players.get("experience") %></td>
                                    <td class="uk-text-center"><%= players.get("strength") %> <span class="uk-badge uk-badge-success uk-badge-notification">+1</span></td>
                                    <td class="uk-text-center"><%= players.get("fitness") %> <span class="uk-badge uk-badge-danger uk-badge-notification">-3</span></td>
                                    <td class="uk-text-center"><%= players.get("morale") %></td>
                                    <td class="uk-text-center"><%= players.get("health") %></td>
                                    <td class="uk-text-center"><%= players.get("teamwork") %></td>
                                    <td class="uk-text-center"></td>
                                    <td class="uk-text-right"><%= players.get("salary") %></td>
                                    <td class="uk-text-right"><%= players.get("price") %></td>
                                    <td class="uk-text-right"><%= players.get("birthtour") %></td>
                                </tr>
                                <% }) %>
                                </tbody>
                                </table>

                                </li>
                                <li>
 
                                </li>
                                <li>
 
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
 
            </div>
        </div>
    </div>



 
    </div>
</div>


<!-- Include the footer partial -->
<% include ../_footer %>
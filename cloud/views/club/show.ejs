<!-- Include the header partial and specify a title -->
<% title = club.get('name') %>
<% include ../_header %>

<!-- content -->
<div id="page_content">
        <div id="page_content_inner">
            <div class="uk-grid" data-uk-grid-margin data-uk-grid-match id="user_profile">
                           <div class="uk-width-large-3-10">
                    <div class="md-card">
                        <div class="user_heading club_heading">
                            <div class="user_heading_menu" data-uk-dropdown>
                                <i class="md-icon material-icons md-icon-light">&#xE5D4;</i>
                                <div class="uk-dropdown uk-dropdown-flip uk-dropdown-small">
                                    <ul class="uk-nav">
                                        <li><a href="#"><%= cDate.getDate()+'.'+cDate.getMonth()+'.'+cDate.getFullYear()  %></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="user_heading_avatar">
                                <img src="http://www.fa13.info/image/logo/<%= club.get('clubId') %>.gif" alt="user avatar"/>
                            </div>
                            <div class="user_heading_content">
                                <h1 class="heading_b">
                                    <span class="uk-text-truncate"><%= club.get('name') %></span>
                                    <span class="sub-heading"><%= club.get('city') %></span>
                                </h1>

 
                            </div>
                        </div>
                        <div class="md-card-content">
                            <ul class="md-list md-list-addon">
                               <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">star_rate</i>
                                    </div>
                                    <div class="md-list-content">
                                        <div class="uk-float-right uk-margin-top uk-margin-small-right">
                                            <span class="peity_sale peity_data">
                                                <% for(var i=0; i<(aClub.length-1); i++) { %><%= aClub[i].get('raiting')+',' %><% } %><%= club.get('raiting') %>
                                            </span>
                                        </div>
                                        
                                        <h2 class="uk-margin-remove"><span class="countUpMe"><%= club.get('raiting') %></span>
                                            <% if(club.get('raiting') - pClub.get('raiting') != 0) { %>
                                            <span class="uk-float-none uk-badge 
                                            <% if(club.get('raiting') - pClub.get('raiting') > 0) { %> uk-badge-success <% } else { %> uk-badge-danger <% }; %> uk-badge-notification">
                                            <% if(club.get('raiting') > pClub.get('raiting')) { %>+<% }; %><%= club.get('raiting') - pClub.get('raiting') %></span><% }; %>
                                        </h2>
                                        <span class="uk-text-small uk-text-muted">Рейтинг</span>

                                    </div>
                                </li>
                               <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">attach_money</i>
                                    </div>
                                    <div class="md-list-content">
                                        <div class="uk-float-right uk-margin-top uk-margin-small-right">
                                            <span class="peity_sale peity_data">
                                                <% for(var i=0; i<(aClub.length-1); i++) { %><%= aClub[i].get('fc')+',' %><% } %><%= club.get('fc') %>
                                            </span>
                                        </div>
                                        
                                        <h2 class="uk-margin-remove"><span class="countUpMe"><%= (club.get('fc') / 1000000).toFixed(2) %></span> <span class="uk-text-small"> млн</span>

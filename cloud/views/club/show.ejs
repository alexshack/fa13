<!-- content -->

    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
 
    <section class="content">
        <div class="row">   
            <div class="col-md-5">
                <div class="box box-widget widget-user-2 club-header">
                <!-- Add the bg color to the header using any of the bg-* classes -->
                    <div class="widget-user-header">
                        <div class="widget-user-image">
                            <img class="club-logo" src="http://www.fa13.info/image/logo/<%= club.get('clubId') %>.gif" alt="<%= club.get('name') %>">
                        </div>
                        <h3 class="widget-user-username"><%= club.get('name') %></h3>
                        <h5 class="widget-user-desc"><a href="#" title="<%= club.get('country') %>"><img src="/dist/img/blank.gif" class="flag flag-<%= club.get('flag').get('flag') %>" alt="<%= club.get('country') %>" /></a>
                                        <a href="#"><span><%= club.get('country') %></span></a>
                        </h5>
                         <div class="widget-user-raiting">
                            <div class="btn-app">
                                <% if (club.get('raiting')!=pClub.get('raiting')) { %>
                                    <% if (club.get('raiting')>pClub.get('raiting')) { %>
                                        <span class="badge bg-green"><%= club.get('raiting')-pClub.get('raiting') %></span>
                                    <% } else {%>
                                        <span class="badge bg-red"><%= club.get('raiting')-pClub.get('raiting') %></span>
                                    <% }; %>
                                <% }; %>

                                <div class="value"><%= club.get('raiting') %></div>
                                <span>Рейтинг</span>
                            </div>
                            <div class="btn-app">
                                <span class="badge bg-green">x.xx</span>
                                <div class="value">xx.xx</div>
                                <span>Сила</span>
                            </div>
                            <div class="btn-app">
                               <% if (club.get('bum')!=pClub.get('bum')) { %>
                                    <% if (club.get('bum')>pClub.get('bum')) { %>
                                        <span class="badge bg-green"><%= club.get('bum')-pClub.get('bum') %></span>
                                    <% } else {%>
                                        <span class="badge bg-red"><%= club.get('bum')-pClub.get('bum') %></span>
                                    <% }; %>
                                <% }; %>

                                <div class="value"><%= club.get('bum') %></div>
                                <span>БУМ</span>
                            </div>
                         </div>

 
                    </div>

                    <div class="box-body">
                        <div class="row">
                            <div class="col-xs-6 border-right">
                                <div class="description-block">
                                    <div id="sparkline-2"></div>
                                    <span class="description-text">Финансы клуба</span>
                                    <h5 class="description-header"><%= (club.get('fc')/1000000).toFixed(2) %> млн.</h5>
                                </div><!-- /.description-block -->
                            </div><!-- /.col -->
                            <div class="col-xs-6">
                                <div class="description-block">
                                    <div id="sparkline-3"></div>
                                    <span class="description-text">Стоимость игроков</span>
                                    <h5 class="description-header"><%= club.get('allPrice') %> млн.</h5>
                                </div><!-- /.description-block -->
                            </div><!-- /.col -->

                        </div><!-- /.row -->
 
                    </div>
                    <div class="box-footer">
                        <div class="row">
                            <div class="col-xs-4 border-right" >
                                <div class="description-block">
                                <input type="text" class="knob" data-readonly="true" value="<%= club.get('stadionState') %>" data-width="60" data-height="60" data-fgColor="
                                <% if (club.get('stadionState')<25) { %>
                                #008d4c
                                <% } else { if (club.get('stadionState')<50) { %>
                                #00a7d0
                                <% } else { if (club.get('stadionState')<75) { %>
                                #db8b0b
                                <% } else { if (club.get('stadionState')<100) { %>
                                #d33724
                                <% }}}} %>
                                
                                ">
                                <h6 class="description-header"><%= club.get('stadionSize') %></h6>
                                <span class="description-text">Стадион</span>
                            </div>
                            </div><!-- ./col -->
                            <div class="col-xs-4 text-center border-right">
                                <div class="description-block">
                                <% if (club.get('base')>0) { %>
                                <input type="text" class="knob" data-readonly="true" value="<%= club.get('baseState') %>" data-width="60" data-height="60" data-fgColor="
                               <% if (club.get('baseState')<25) { %>
                                #008d4c
                                <% } else { if (club.get('baseState')<50) { %>
                                #00a7d0
                                <% } else { if (club.get('baseState')<75) { %>
                                #db8b0b
                                <% } else { if (club.get('baseState')<100) { %>
                                #d33724
                                <% }}}} %>
                                ">
                                <h6 class="description-header">Уровень <%= club.get('base') %></h6>
                                <% } else { %>
                                <canvas width="60" height="60"></canvas>
                                <h6 class="description-header">Не построена</h6>
                                <% } %>
                                <span class="description-text">Спортбаза</span>
                            </div>
                            </div><!-- ./col -->
                            <div class="col-xs-4 text-center">
                                <div class="description-block">
                                <% if (club.get('school')>0) { %>
                                <input type="text" class="knob" data-readonly="true" value="<%= club.get('schoolState') %>" data-width="60" data-height="60" data-fgColor="
                               <% if (club.get('schoolState')<25) { %>
                                #008d4c
                                <% } else { if (club.get('schoolState')<50) { %>
                                #00a7d0
                                <% } else { if (club.get('schoolState')<75) { %>
                                #db8b0b
                                <% } else { if (club.get('schoolState')<100) { %>
                                #d33724
                                <% }}}} %>
                                ">
                                <h6 class="description-header">Построена</h6>
                                <% } else { %>
                                <canvas width="60" height="60"></canvas>
                                <h6 class="description-header">Не построена</h6>
                                <% } %>
                                <span class="description-text">Спортшкола</span>
                            </div>
                            </div><!-- ./col -->
                        </div><!-- /.row -->
                    </div><!-- /.box-footer -->
                   <div class="box-footer no-padding">
                        <div class="row">
                            <div class="col-xs-6">
                                <ul class="nav nav-pills nav-stacked">
                                    <li><a href="#">Игроки <span class="pull-right"> <%= players.length %></span></a></li>
                                    <li><a href="#">Средний возраст <span class="pull-right"> <%= (club.get('avgAge')/1).toFixed(0) %></span></a></li>
                                    <li><a href="#">Средний талант <span class="pull-right"> <%= (club.get('avgTallant')/1).toFixed(0) %></span></a></li>
                                    <li><a href="#">Средняя сила <span class="pull-right "> <%= (club.get('avgStrength')/1).toFixed(0) %></span></a></li>
                                    <li><a href="#">ЗП игроков <span class="pull-right"> <%= (club.get('allSalary')/1000).toFixed(2) %> млн.</span></a></li>
                                </ul>
                            </div>
                            <div class="col-xs-6">
                                <ul class="nav nav-pills nav-stacked">
                                    <li><a href="#">КГТ <span class="pull-right"> <%= (club.get('coach')-200).toFixed(0) %></span></a></li>
                                    <li><a href="#">Доктор <span class="pull-right "> <%= club.get('doctorPlayers') %>/<%= club.get('doctorQualification') %></span></a></li>
                                    <li><a href="#">Скаут <span class="pull-right"> <%= club.get('scout') %></span></a></li>
                                    <li><a href="#">Тренера <span class="pull-right"> <%= club.get('goalkeepersCoach') %>/<%= club.get('defendersCoach') %>/<%= club.get('midfieldersCoach') %>/<%= club.get('forwardsCoach') %>/<%= club.get('fitnessCoach') %>/<%= club.get('moraleCoach') %></span></a></li>
                                    <li><a href="#">ЗП тренеров <span class="pull-right"> <%=(club.get('coachSallary')<1000?(club.get('coachSallary')/1).toFixed(2) + " тыс.":(club.get('coachSallary')/1000).toFixed(2) + " млн.") %></span></a></li>
                                </ul>
                            </div>
                        </div>
                    </div><!-- /.footer -->
                    <div class="box-footer text-center">
                        <a href="javascript::"><span class="uppercase text-light-blue">Подробная информация о клубе  </span> </a>
                    </div>
                </div><!-- /.box -->
                <% if (club.get('manager')) { %>
                <div class="box box-widget widget-user-2 club-header manager-header box-primary">
                <!-- Add the bg color to the header using any of the bg-* classes -->
                                    <div class="box-header with-border">
                                        <h3 class="box-title">Менеджер клуба <%= club.get('name') %></h3>
                                                                <div class="box-tools pull-right">
                            <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                                    </div>
                    <div class="widget-user-header box-body">
                        <div class="widget-user-image">
                            <img class="club-logo" src="" alt="<%= club.get('manager').get('name') %>">
                        </div>
                        <h3 class="widget-user-username"><%= club.get('manager').get('name') %></h3>
                        <h5 class="widget-user-desc"><img src="/dist/img/blank.gif" class="flag flag-<%= club.get('manager').get('flag').get('flag') %>" alt="<%= club.get('manager').get('country') %>" /><span><%= club.get('manager').get('country') %>, <%= club.get('manager').get('city')%></span>
                        </h5>
                         <div class="widget-user-raiting">
                             <div class="btn-app">
                                <div class="value"><%= club.get('manager').get('matches') %></div>
                                <span>Матчи</span>
                            </div>
                            <div class="btn-app">
                                <div class="value"><%= club.get('manager').get('fm') %></div>
                                <span>ФМ</span>
                            </div> 
                         </div>

 
                    </div>
                </div>
                <% }; %>
            </div>
            <div class="col-md-7">
                <div class="box box-primary">
                    <div class="box-header ">
                        <h3 class="box-title">Состав клуба <%= club.get('name') %></h3>
                        <div class="box-tools pull-right">
                            <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>

                    </div><!-- /.box-header -->
                    <div class="box-body table-responsive no-padding">
                        <table class="table table-hover">
                            <tbody>
                                <tr>
                                    <th class="text-center">#</th>
                                    <th class="text-center">ПОЗ</th>
                                    <th>ИГРОК</th>
                                    <th class="text-center">НАЦ</th>
                                    <th class="text-right">ВОЗ</th>
                                    <th class="text-right">ТАЛ</th>
                                    <th class="text-right">СИЛ</th>
                                    <th class="text-right">З/П</th>
                                    <th class="text-right">ЦЕНА</th>
                                    <th class="text-right">&nbsp;</th>
                                </tr>
                                <% players.forEach(function(player) { %>
                                <tr>
                                    <td class="text-center"><%= player.get('number') %></td>
                                    <td class="text-center"><%= player.get('position') %></td>
                                    <td><a href="/player/<%= player.get('playerId') %>"><%= player.get('name') %></a><% if (player.get('disqualification')>0) { %> <i class="fa fa-ban text-danger" title="Дисквалификация: <%= player.get('disqualification') %>"></i><% } %><% if (player.get('fitness')<71) { %> <i class="fa fa-medkit text-danger" title="Физа: <%= player.get('fitness') %>"></i><% } %></td>
                                    <td class="text-center">
                                        <img src="/dist/img/blank.gif" class="flag flag-<%= player.get('nationalityCode').get('flag') %>" alt="<%= player.get('nationality') %>" title="<%= player.get('nationality') %>" />
                                    </td>
                                    <td class="text-right"><%= player.get('age') %></td>
                                    <td class="text-right"><%= player.get('talent') %></td>
                                    <td class="text-right"><%= player.get('strength') %></td>
                                    <td class="text-right"><%= player.get('salary') %></td>
                                    <td class="text-right"><%= player.get('price') %>
 

                                    </td>
                                    <td>&nbsp;</td>
                                </tr>
                                <% }) %>
                         </tbody>
                        </table>
                    </div><!-- /.box-body -->
                    <div class="box-footer text-center">
                        <a href="javascript::"><span class="uppercase text-light-blue">Подробная информация о составе  </span> </a>
                    </div>
                </div>
            </div>
        </div><!-- /.row -->
    </section>

        <!-- Main content -->

    </div><!-- /.content-wrapper -->



<script>
$(".knob").knob();
var myvalues = [<% for (var i=aClub.length-1; i!=-1; i--){ %><%= aClub[i].get('fc') %>,<% }; %>];
  $('#sparkline-2').sparkline(myvalues, {
    type: 'line',
    lineColor: '#92c1dc',
    fillColor: "#ebf4f9",
    height: '50',
    width: '80'
  });
  myvalues = [15, 19, 20, 22, 33, 27, 31, 27, 19, 20, 21];
  $('#sparkline-3').sparkline(myvalues, {
    type: 'line',
    lineColor: '#92c1dc',
    fillColor: "#ebf4f9",
    height: '50',
    width: '80'
  });


</script>

<script>

    var matches = <%- JSON.stringify(matches) %>;
    console.log(matches);
</script>



